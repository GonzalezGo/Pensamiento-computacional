import math


print("Primer programa")
num1= input("Ingrese un número ")
num2= input("Ingrese segundo número ")
num1= int(num1)
num2= int(num2)

suma = num1+num2
print(suma)

raíz = math.sqrt(num1)
print("La raíz cuadrada es " + str(raíz)) 

potencia = num1 ** num2 
print("La potencia es  " + str(potencia))

print("Segundo programa")
num1= input("Ingrese un número ")
num2= input("Ingrese segundo número ")
num1= int(num1)
num2= int(num2)

menos = num1-num2
print(menos)

print("Tercer programa")
num1= input("Ingrese un número ")
num2= input("Ingrese segundo número ")
num1= int(num1)
num2= int(num2)

multiplicar = num1*num2
print(multiplicar)

print("Cuarto programa")
num1= input("Ingrese un número ")
num2= input("Ingrese segundo número ")
num1= int(num1)
num2= int(num2)

división = num1/num2
print(división)

